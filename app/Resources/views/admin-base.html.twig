<!DOCTYPE html>
<html lang="pl">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{% endblock %}{{ 'base.admin.title'|trans({},'controller') }}</title>
    <link rel="icon" type="image/png" href="{{ asset('favicon.ico') }}">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
          rel="stylesheet">
    {% stylesheets
    'css/bootstrap.min.css'
    'css/main.css'
    filter='?uglifycss'
    filter='cssrewrite'
    output='css/compiled.css' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
</head>
<body>
<div class="line">
</div>

<nav class="navbar navbar-expand-sm bg-dark navbar-dark">
    <a class="navbar-brand" href="{{ path('admin_panel') }}">{{ 'base.admin.panel_name'|trans({},'controller') }}</a>

    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsibleNavbar">
        <span class="navbar-toggler-icon"></span>
    </button>

    {#<div class="collapse navbar-collapse" id="collapsibleNavbar">#}
        {#<ul class="navbar-nav">#}
            {#<li class="nav-item active">#}
                {#<a class="nav-link" href="#">Main</a>#}
            {#</li>#}
            {#<li class="nav-item">#}
                {#<a class="nav-link" href="#">Posts</a>#}
            {#</li>#}
            {#<li class="nav-item">#}
                {#<a class="nav-link" href="#">Category</a>#}
            {#</li>#}
            {#<li class="nav-item">#}
                {#<a class="nav-link" href="#">Tags</a>#}
            {#</li>#}
            {#<li class="nav-item">#}
                {#<a class="nav-link" href="#">Users</a>#}
            {#</li>#}
        {#</ul>#}
    {#</div>#}

    {{ print_admin_menu() }}
    <div class="ml-lg-auto">
        <a href="{{ path('blog_index') }}" class="btn-primary btn">{{ 'base.admin.front_page'|trans({},'controller') }}</a>

    </div>
</nav>
<div class="container ">
{% block content %}

{% endblock %}
</div>

</body>
<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
        integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
