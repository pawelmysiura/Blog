<div class="collapse navbar-collapse" id="collapsibleNavbar">
    <ul class="navbar-nav">
    {% for name, route in adminMenu %}
        {% if app.request.get('_route') == route %}
            <li class="nav-item active">
        {% else %}
            <li class="nav-item">
        {% endif %}
        <a class="nav-link" href="{{ path(route) }}">{{ name }}</a>
        </li>
        {% endfor %}
    </ul>
</div>